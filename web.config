<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="iisnode" path="server.js" verb="*" modules="iisnode" />
    </handlers>
    <rewrite>
      <rules>
      <rule name="DynamicContent">
           <match url="/*" />
                <conditions>
                        <add input="{URL}" pattern="^/apiGateway/(.*)" negate="true" />
                        <add input="{URL}" pattern="^/apiGateway2/(.*)" negate="true" />
                        <add input="{URL}" pattern="^/257359.txt" negate="true" />
                        <add input="{URL}" pattern="^/997797.txt" negate="true" />
                </conditions>
                <action type="Rewrite" url="server.js" />
		  </rule>
      </rules>
    </rewrite>
    <iisnode watchedFiles="web.config;*.js" />
  </system.webServer>
</configuration>
